@model DuzceUniversitesi_Model.Models.AkademikPersonel
@{
    ViewData["Title"] = "YeniPersonel";
    Layout = "~/Views/Shared/_AdminPanelLayout.cshtml";
}

<h1>Yeni Duyuru Ekle</h1>
<br />
<form class="form-group" method="post">
    @Html.LabelFor(x => x.AkademikPersonelAdSoyad)
    @Html.TextBoxFor(x => x.AkademikPersonelAdSoyad, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.AkademikPersonelDogumTarihi)
    @Html.TextBoxFor(x => x.AkademikPersonelDogumTarihi, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.AkademikPersonelDersler)
    @Html.TextAreaFor(x => x.AkademikPersonelDersler, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.AkademikPersonelMail)
    @Html.TextBoxFor(x => x.AkademikPersonelMail, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.AkademikPersonelGorsel)
    @Html.TextBoxFor(x => x.AkademikPersonelGorsel, new { @class = "form-control" })
    <br />

    <button class="btn btn-success">Akademik Personel Ekle</button>
</form>

